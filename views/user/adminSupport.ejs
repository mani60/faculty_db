<% include ../partials/header %>

<!--NAVIGATION BAR FOR ADMIN-->
<div class="container-fluid" style="padding: 0; margin: 0;">
  <div class="header-container">
    <img class="header-image" src="https://www.amrita.edu/sites/default/files/logo.png">
          <!-- https://www.pngitem.com/pimgs/m/539-5392274_amrita-university-logo-hd-png-download.png -->
          <h1 class="header-text">Faculty Dashboard</h1>
      </div>
    </div>
    </div>
<div class="container my-4">
    <div class="row justify-content-center">
        <div class="col-md-7">
          <div class="card transparent-container">
            <div class="card-header text-center login-card-header">
                <h4 class="h3-login login-h4">Admin Support</h4>
                <hr class="hr-login">
              </div>
                <div class="card-block">
                  <form class="p-3" action="/auth/admin-support" method="POST">
                      
                    <div class="form-group">
                      <label class="form-control-label">Name</label>
                      <input type="text" name="name" placeholder="Name" class="form-control" required>
                    </div>

                    <div class="form-group">
                      <label class="form-control-label">Roll Number</label>
                      <input type="text" name="roll" placeholder="Roll Number" class="form-control" required>
                    </div>
                    
                    <div class="form-group">
                      <label class="form-control-label">Title of query</label>
                      <input type="text" name="title"  class="form-control" required>
                    </div>

                    <div class="form-group">
                      <label class="form-control-label">Query description</label>
                      <textarea name="description" cols="30" rows="10" class="form-control"></textarea>
                    </div>
                    <button type="submit"class="addc-btn" class="btn btn-warning btn-block">Submit</button>
                  </form>
                </div>
            </div>
        </div>
    </div>
</div>


<script src="https://cdn.ckeditor.com/4.9.2/standard/ckeditor.js"></script>
<% include ../partials/footer %>
